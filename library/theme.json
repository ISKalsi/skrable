{
  "panelOverlay": {
    "colours": {
      "dark_bg": "#00000099"
    }
  },
  "overlay": {
    "colours": {
      "dark_bg": "#00000055"
    }
  },
  "#wordPanel": {
    "font": {
      "name": "roboto",
      "size": "25",
      "regular_path": "library/Roboto-Black.ttf"
    }
  },
  "wordLabel": {
    "font": {
      "name": "roboto-black",
      "size": "25",
      "regular_path": "library/Roboto-Black.ttf"
    }
  },
  "timeLabel": {
    "font": {
      "name": "roboto-regular",
      "size": "25",
      "regular_path": "library/Roboto-Regular.ttf"
    }
  },
  "#playerLabel": {
    "colours": {
      "dark_bg": "#00000000"
    }
  },
  "playerRank": {
    "prototype": "#playerLabel",
    "font": {
      "name": "roboto-black",
      "size": "30"
    }
  },
  "playerName": {
    "prototype": "#playerLabel",
    "font": {
      "name": "roboto-medium",
      "size": "23",
      "regular_path": "library/Roboto-Medium.ttf"
    }
  },
  "playerScore": {
    "prototype": "#playerLabel",
    "font": {
      "name": "roboto-light",
      "size": "14",
      "regular_path": "library/Roboto-Light.ttf"
    }
  },
  "roundLabel": {
    "prototype": "#playerLabel",
    "font": {
      "name": "roboto-black",
      "size": "30"
    }
  },
  "roundPanel": {
    "misc": {
      "border_width": "0",
      "shadow_width": "0"
    }
  },
  "scoreboard": {
    "font": {
      "name": "roboto-regular",
      "size": "35"
    },
    "colours":{
      "dark_bg": "#33445500"
    }
  },
  "scoreboardName": {
    "prototype": "scoreboard",
    "font": {
      "name": "roboto-light",
      "size": "27"
    }
  },
  "scoreboardDeltaScore": {
    "prototype": "scoreboard",
    "font": {
      "name": "roboto-light",
      "size": "27"
    }
  }
}